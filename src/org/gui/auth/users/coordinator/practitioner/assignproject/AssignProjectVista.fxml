                        <Label fx:id="practitionerNameLabel" alignment="CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="35.0" prefWidth="469.0" styleClass="secondLevelLabel" text="NOMBRE DEL PRACTICANTE" textAlignment="CENTER" wrapText="true">
